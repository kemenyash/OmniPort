{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "UploadLimits": {
      "MaxUploadBytes": 209715200, // 200 MB
      "InMemoryThresholdBytes": 33554432, // 32 MB
      "PerType": {
        "Excel": 104857600, // 100 MB
        "Csv": 52428800, // 50 MB
        "Json": 20971520, // 20 MB
        "Xml": 20971520 // 20 MB
      }
    },
  "AllowedHosts": "*"
}
